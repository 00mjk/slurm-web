<div class="content">
  <div id="qos-list">
    {{#each jobs.qos}}
      <div class="qos" data-id="{{id}}" title="QoS {{id}}" style="height:{{height}}px;line-height:{{height}}px;">
        {{id}}
      </div>
    {{/each}}
  </div>
  <div id="jobs-chart">
    <div class="wrapper" style="width:{{jobs.width}}px;height:{{jobs.height}}px;">
      <div class="current" style="height:{{jobs.height}}px;left:{{jobs.currentPosition}}%;"></div>
      {{#each jobs.jobs}}
        <div class="job" data-id="{{id}}" title="Job #{{id}}"
            style="height:{{height}}px;line-height:{{height}}px;top:{{positionY}}px;width:{{width}}%;left:{{positionX}}%;background-color:{{color}}">
          <span data-id="{{id}}">{{id}}</span>
        </div>
      {{/each}}
    </div>
  </div>
</div>
<div id="time">
  <div class="wrapper" style="width:{{jobs.width}}px;height:50px;">
    {{#each jobs.times}}
      <div class="time" style="left:{{position}}%;">
        {{value}}
      </div>
    {{/each}}
  </div>
</div>
